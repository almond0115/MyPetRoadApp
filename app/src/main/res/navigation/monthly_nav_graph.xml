    android:id="@+id/monthly_nav_graph"
    app:startDestination="@id/analysisFragment">

    <fragment
        android:id="@+id/analysisDetailFragment"
        android:name="com.mju.capstone.mypetRoad.views.feature.analysis.AnalysisDetailFragment"
        android:label="AnalysisDetailFragment" >
        <action
            android:id="@+id/action_analysisDetailFragment_to_analysisFragment"
            app:destination="@id/analysisFragment" />
    </fragment>
    <fragment
        android:id="@+id/analysisFragment"
        android:name="com.mju.capstone.mypetRoad.views.feature.analysis.AnalysisFragment"
        android:label="AnalysisFragment" >
        <action
            android:id="@+id/action_analysisFragment_to_analysisDetailFragment"
            app:destination="@id/analysisDetailFragment" />
    </fragment>
