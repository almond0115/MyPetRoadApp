<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".views.feature.analysis.EntireFragment">

            <include
                android:id="@+id/top_card"
                layout="@layout/entirelog_card" />

            <TextView
                android:id="@+id/my_walking_map_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/entire_walking_map"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/top_card"
                app:layout_constraintStart_toStartOf="parent"/>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/my_walking_log"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:overScrollMode="never"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/my_walking_map_text" />

            <TextView
                android:id="@+id/my_walking_log_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/entire_walking_log"
                android:textColor="@color/black"
                android:textSize="15dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/my_walking_log"
                app:layout_constraintStart_toStartOf="parent"/>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/entire_walking_log"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:overScrollMode="never"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@id/my_walking_log_text"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</layout>